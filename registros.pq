// Exemplo de registro
let
    cliente = [
        Nome = "Maria Oliveira",
        Idade = 28,
        Email = "maria@exemplo.com",
        Premium = true
    ],
    
    // Acessando campos
    nomeCliente = cliente[Nome],       // "Maria Oliveira"
    idadeCliente = cliente[Idade],     // 28
    emailCliente = cliente[Email]      // "maria@exemplo.com"
in
    nomeCliente